{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "esnext",
    "module": "es2015",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "moduleResolution": "node",
    "noImplicitAny": false,
    "noImplicitReturns": false,
    "lib": [
      "esnext"
    ],
    "typeRoots": ["types", "node_modules/@types"],
    "types": ["node", "jest"],
    "paths": {"promise-mysql": ["types/promise-mysql"]}
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
